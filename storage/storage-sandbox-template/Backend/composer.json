{
    "name": "api/project",
    "description": "API project with PSR-4 autoloading",
    "require": {
        "php": ">=7.4",
        "zircote/swagger-php": "^5.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "",
            "App\\Commons\\": "Commons/",
            "App\\Functions\\": "Functions/",
            "App\\Routes\\": "Routes/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "Tests/"
        }
    }
}
